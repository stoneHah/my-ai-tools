# AI中台服务配置
# 将此文件复制为.env并填入您的实际配置

# Coze服务配置
COZE_API_TOKEN=your_coze_api_token_here
COZE_API_BASE=https://api.coze.cn  # 中国区使用此URL，国际区使用https://api.coze.com
COZE_DEFAULT_BOT_ID=your_default_bot_id_here  # 可选，默认使用的Bot ID
COZE_DEFAULT_WORKFLOW_ID=your_default_workflow_id_here  # 可选，默认使用的工作流ID

# ASR服务配置
DASHSCOPE_API_TOKEN=your_dashscope_api_token_here

# 默认语音识别模型
DEFAULT_ASR_SERVICE=paraformer-v2

# 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=123456
MYSQL_DB_NAME=ai_tools

# 火山引擎TTS配置
VOLCENGINE_TTS_APPID=your_volcengine_tts_appid_here
VOLCENGINE_TTS_TOKEN=your_volcengine_tts_token_here
VOLCENGINE_TTS_CLUSTER=your_volcengine_tts_cluster_here

# 阿里云OSS配置
ALIYUN_OSS_ENDPOINT=your_aliyun_oss_endpoint_here
ALIYUN_OSS_ACCESS_KEY_ID=your_aliyun_oss_access_key_id_here
ALIYUN_OSS_ACCESS_KEY_SECRET=your_aliyun_oss_access_key_secret_here
ALIYUN_OSS_BUCKET_NAME=your_aliyun_oss_bucket_name_here

# 服务器配置
PORT=8000
HOST=0.0.0.0



# 其他AI服务配置可在此添加
# 例如：OpenAI、百度文心、讯飞星火等
